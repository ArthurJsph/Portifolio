
{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/src/main.jsx",
      "dest": "/index.html"
    },
    {
      "src": "/.well-known/*",
      "status": 200,
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
